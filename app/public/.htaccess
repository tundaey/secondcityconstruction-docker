RewriteEngine On

RewriteCond %{HTTP_HOST} ^website-ssrc.rhcloud.com$
RewriteRule ^(.*)$ http://secondcityconstruction.com/$1 [R=301,L] 

RewriteBase /
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond $0#%{REQUEST_URI} ([^#]*)#(.*)\1$
RewriteRule ^.*$ %2index.php [QSA,L]


